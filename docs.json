{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Rubic Documentation",
  "colors": {
    "primary": "#00e28e",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "API Reference",
        "openapi": "https://api-v2.rubic.exchange/api/routes/swagger-json"
      },
      {
        "tab": "API Documentation",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "docs/api-docs/get-crosschain-status",
              "docs/api-docs/supported-chains",
              "docs/api-docs/supported-providers",
              "docs/api-docs/request-quote",
              "docs/api-docs/request-data",
              "docs/api-docs/integration-examples",
              "docs/api-docs/swagger",
              "docs/api-docs/models",
              "docs/api-docs/errors",
              "docs/api-docs/dangerous-routes"
            ]
          },
          {
            "group": "Monetization and Fees",
            "pages": [
              "docs/monetization/monetization",
              "docs/monetization/evm",
              "docs/monetization/non-evm"
            ]
          },
          {
            "group": "Referrer and Rate Limit",
            "pages": [
              "docs/referrer/referrer"
            ]
          },
          {
            "group": "Tokens API",
            "pages": [
              "docs/token-api/token-api"
            ]
          },
          {
            "group": "Mintlify Settings",
            "pages": [
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "Mintlify Edits",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "Mintlify AI",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://mintlify.com/community",
          "icon": "slack"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "rubic-logo.svg",
    "dark": "rubic-logo.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
